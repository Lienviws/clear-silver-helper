{
    "comments": {
        // symbols used for start and end a block comment. Remove this entry if your language does not support block comments
        "blockComment": ["<?cs #", "?>"]
    },
    // 对称高亮
    "brackets": [
        ["<?", "?>"],
		["{", "}"],
		["(", ")"]
    ],
    // 输入的时候自动填充
    "autoClosingPairs": [
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // 选中时，输入以下符号，自动将选中范围用对称符号包围起来
    "surroundingPairs": [
        { "open": "'", "close": "'" },
		{ "open": "\"", "close": "\"" },
		{ "open": "[", "close": "]"},
		{ "open": "(", "close": ")" }
    ],
    // 折行
    "folding": {
        "markers": {
            "start": "<\\?cs\\s+#\\s+region\\b.*\\?>",
            "end": "<\\?cs\\s+#\\s+endregion\\b.*\\?>"
        }
    }
}