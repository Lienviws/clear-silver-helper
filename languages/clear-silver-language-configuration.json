{
    "comments": {
        // symbols used for start and end a block comment. Remove this entry if your language does not support block comments
        "blockComment": ["<?cs #", "?>"]
    },
    // 对称高亮
    "brackets": [
        ["<", ">"],
        ["?cs", "?"],
        ["(", ")"]
    ],
    // 输入的时候自动填充
    "autoClosingPairs": [
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // 双击高亮范围
    "surroundingPairs": [
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // 折行
    "folding": {
        "markers": {
            "start": "^<\\?cs\\s(if|with|loop|alt):[a-zA-Z0-9.#]+\\s\\?>",
            "end": "^<\\?cs\\s/(if|with|loop|alt)\\s\\?>"
        }
    }
}